<%= form_with model: event do |f| %>
  <%= f.label :name, "Name" %> <br>
  <%= f.text_field :name %> <br>
  <%= f.label :description, "Description" %> <br>
  <%= f.text_area :description %> <br>
  <%= f.label :location, "Location" %> <br>
  <%= f.text_field :location %> <br>
  <%= f.label :date, "Date" %>
  <%= f.date_select :date %> <br>
  <%= f.submit %> <br>
<% end %>
